{
  "expo": {
    "name": "UrbanFix",
    "slug": "urbanfix-citizen",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "android": {
      "package": "com.nextgen.urbanfix",
      "adaptiveIcon": {
        "backgroundColor": "#2563eb"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.nextgen.urbanfix",
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow UrbanFix to access camera to report civic issues"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow UrbanFix to use your location to accurately report civic issues"
        }
      ],
      "expo-font"
    ]
  }
}
